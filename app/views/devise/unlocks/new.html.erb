<h2><%= f.submit t('.header') %></h2>

<%= form_with(model: resource, url: unlock_path(resource_name), data: { turbo: false }) do |f| %>

  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="actions">
    <%= f.submit t('.header') %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
